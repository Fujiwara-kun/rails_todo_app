<h1>新規作成</h1>
<%= form_with model: @task,local: true do |form| %>
    <div>
        <%= form.label :title, "タスク名" %><br>
        <%= form.text_field :title %>
    </div>

    <div>
        <%= form.label :done, "完了" %>
        <%= form.check_box :done %>
    </div>
    <div>
        <%= form.submit "作成"%>
    </div>
<% end %>

<%= link_to "戻る",tasks_path %>